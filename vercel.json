{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env." {
    "APP_ENV": "production"
    "APP_DEBUG": "false"
    "APP_URL": "https://umkm-pws.vercel.app"
    "APP_KEY": "base64:YourBase64EncodedKeyHere"
    "LOG_CHANNEL": "stderr"
    "CACHE_DRIVER": "ararry"
    "SESSION_DRIVER": "cookie"
  }
}
